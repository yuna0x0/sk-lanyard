{
	"name": "sk-lanyard",
	"version": "0.3.0",
	"description": "SvelteKit integration with Lanyard, an API to fetch your Discord presence",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"repository": "https://github.com/yuna0x0/sk-lanyard",
	"contributors": [
		"Nebula <infinitewormhole@gmail.com>",
		"yuna0x0 <yuna@yuna0x0.com>"
	],
	"bugs": {
		"url": "https://github.com/yuna0x0/sk-lanyard/issues"
	},
	"files": [
		"dist",
		"package.json"
	],
	"keywords": [
		"typescript",
		"svelte",
		"sveltekit",
		"lanyard",
		"discord",
		"presence"
	],
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "ncc build src/index.ts -m -e svelte/store",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"up": "tsup src/index.ts --dts --format esm,cjs --external svelte/store",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"@vercel/ncc": "^0.38.2",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-svelte": "^2.46.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"svelte": "^4.2.19",
		"vite": "^5.4.10"
	},
	"dependencies": {
		"vite": "^5.4.10"
	}
}
